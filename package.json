{
  "name": "@bluehawks/cli",
  "version": "1.0.31",
  "description": "A production-ready multi-agent AI CLI assistant",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "bluehawks": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "cli",
    "assistant",
    "agent",
    "llm",
    "coding",
    "terminal"
  ],
  "author": "Bluehawks AI",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "eventsource-parser": "^3.0.0",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^15.0.6",
    "marked-terminal": "^7.2.1",
    "ora": "^8.1.1",
    "react": "^18.3.1",
    "simple-git": "^3.27.0",
    "strip-ansi": "^7.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}